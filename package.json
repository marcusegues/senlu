  "dependencies": {
    "@devexpress/dx-react-core": "^1.1.2",
    "@devexpress/dx-react-grid": "^1.1.2",
    "@devexpress/dx-react-grid-material-ui": "^1.1.2",
    "@material-ui/icons": "^1.0.0-beta.43",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "material-ui": "^1.0.0-beta.40",
    "material-ui-icons": "^1.0.0-beta.36",
    "moment": "^2.22.1",
    "npm": "^5.0.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.69.0",
    "prettier": "1.11.1",
    "react-scripts": "^1.1.4"
  },
  "scripts": {
    "start": "node ./server.js",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "npm run build &&  gcloud app deploy",
    "test": "react-scripts test --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "test:coverage": "react-scripts test --coverage",
    "test:coverage:watch": "react-scripts test --watchAll --coverage",
    "test:coverage:ci": "react-scripts test --coverage --maxWorkers 2",
    "lint": "eslint src",
    "lint:tough": "eslint src --max-warnings=0",
    "lint:debug": "eslint src --debug",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js' -x 'src/**/__tests__/**' -t html -t json -t text --threshold 90",
    "eject": "react-scripts eject"
  }
}
